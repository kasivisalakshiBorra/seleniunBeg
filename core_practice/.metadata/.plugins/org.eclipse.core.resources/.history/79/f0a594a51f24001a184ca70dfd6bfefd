	MyThreadImp()
	{
		//start();
		Thread t= new Thread(this);
		t.start();
		} 
	public void run()
	{
		Thread th = Thread.currentThread();
		ThreadGroup tg= th.getThreadGroup();
		System.out.println("Thread Name :"+th.getName()); 
		System.out.println("Thread Group Name :"+tg.getName());
		}
	}
	
	
	

